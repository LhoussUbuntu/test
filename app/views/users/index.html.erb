<h1>Users</h1>

<table class="table">
   <% @users.each do |user| %>
   <thead>
      <tr>
         <th scope="col">#</th>
         <th scope="col">email</th>
      </tr>
   </thead>
   <tbody>
      <tr>
         <th scope="row"><%= user.id %></th>
         <td><%= user.email%></td>
      </tr>
   </tbody>
  <% end %>
</table>